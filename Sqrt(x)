class Solution {
public:
    int mySqrt(int x) {
      long long low=1, high=x;
      long long mid=0;
     while(low<=high){
        mid=(low+high)/2;
        if(mid*mid==x)return mid;
        else if(mid*mid<x){
            low=mid+1;
        }else{
            high=mid-1;
        }
     }  
     return high;
    }
};
